@model List<TokenViewModel>

<div class="table-responsive">
	<table class="table table-bordered">
		<thead>
			<tr>
				<th scope="col">Rank</th>
				<th scope="col">Symbol</th>
				<th scope="col">Name</th>
				<th scope="col">Contract Address</th>
				<th scope="col">Total Holders</th>
				<th scope="col">Total Supply</th>
				<th scope="col">Total Supply %</th>
				<th scope="col"></th>
			</tr>
		</thead>
		<tbody>
			@{
				int rank = 0;
			}
			@for (int i = 0; i < Model.Count(); i++)
			{
				<tr>
					<th scope="row">@(rank++ + 1)</th>
					<td>
						<a href="#" class="a__symbol" token-id="@Model[i].id">@Model[i].symbol</a>
					</td>
					<td>@Model[i].name</td>
					<td>@Model[i].contract_address</td>
					<td>@Model[i].total_holders</td>
					<td>@Model[i].total_supply</td>
					<td>@Model[i].total_supply_perc%</td>
					<td>
						<a href="#" class="a__edit" row-index="@i">Edit</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>